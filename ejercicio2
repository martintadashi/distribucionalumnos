java -jar $RUTA_AL_JAR$NOMBRE_JAR -from $RUTA_AL_PARAMS$NOMBRE_PARAMS
#Muevo la solucion la ruta que nos dieron al la ruta que lo piden
echo "Muevo solucion.csv a "$3
mv solucion.csv $3


